<xerawdp>
	<global>
		<verbosity>1</verbosity> <!-- 0: none, 1: minimal, 2: extensive, 3: debug, 4: too much -->
		<dataset_name_format>nerix_YYMMDD_hhmm_NNNNNN</dataset_name_format>
		<raw_data_dir>/data/nerix/run_16/</raw_data_dir>
		<merged_data_dir>/data/nerix/merged/run_16/</merged_data_dir>
		<processed_data_dir>/data/nerix/merged/run_16/</processed_data_dir>
		<processing_level>0</processing_level>
	</global>
	<pmts>
		<top_pmts>1-16</top_pmts>
		<bottom_pmts>17-17</bottom_pmts>
		<external_gains>1-16:10.00, 17:5.0, 18:0.5</external_gains>
		<calibration_source>
			<here>
				<header>gain gain_err qe</header>
				<values>1-24:[2.00e+6 1.00e+6 2.50e-1]</values>  
			</here>
		</calibration_source>
	</pmts>
	<single_photoelectron>
		<nb_baseline_samples>64</nb_baseline_samples>
		<window_start>64</window_start>
		<window_width>140</window_width>
		<pre_peak_samples>5</pre_peak_samples>
		<post_peak_samples>5</post_peak_samples>
		<integral_window>1:[165 181], 2:[165 181], 3:[167 179], 4:[165 181], 5:[165 181], 6:[165 181], 7:[163 183], 8:[168 178], 9:[168 178], 10:[167 179], 11:[165 181], 12:[165 181], 13:[166 180], 14:[166 180], 15:[168 178], 16:[169 177], 17:[170 176], 18:[170 176]</integral_window>
	</single_photoelectron>
</xerawdp>

